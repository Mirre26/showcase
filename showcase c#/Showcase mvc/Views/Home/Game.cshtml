@model Showcase_mvc.Data.Entities.MinesweeperGame

@{
    ViewData["Title"] = "Minesweeper Game";
}

<h1>@ViewData["Title"]</h1>

<button id="newGameButton">New Game</button>

<div id="gameContainer">
    
</div>



<div id="timer"></div>
<h2 id="timerContent">00:00</h2>


<hr />
<div class="container">
   
    <h3>Highscores</h3>
    <div class="mt-3">
        <ul id="discussion"></ul>
    </div>
    <button id="lastbutton" style="display:none; background: rgb(250, 241, 228); border-color: rgb(250, 241, 228);"></button>

</div>

<script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>
<script src="~/js/chat.js"></script>
<script src="~/js/timer.js"></script>
<script src="~/js/minesweeper.js"></script>



